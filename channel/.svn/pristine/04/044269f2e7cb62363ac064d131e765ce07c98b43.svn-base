[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
[remote "origin"]
	url = https://github.com/symfony/http-kernel.git
	fetch = +refs/heads/*:refs/remotes/origin/*
	pushurl = git@github.com:symfony/http-kernel.git
[branch "master"]
	remote = origin
	merge = refs/heads/master
[remote "composer"]
	url = https://github.com/symfony/http-kernel.git
	fetch = +refs/heads/*:refs/remotes/composer/*
[branch "2.8"]
	remote = composer
	merge = refs/heads/2.8
