[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
[remote "origin"]
	url = https://github.com/thephpleague/url.git
	fetch = +refs/heads/*:refs/remotes/origin/*
	pushurl = git@github.com:thephpleague/url.git
[branch "3.x"]
	remote = origin
	merge = refs/heads/3.x
[remote "composer"]
	url = https://github.com/thephpleague/url.git
	fetch = +refs/heads/*:refs/remotes/composer/*
