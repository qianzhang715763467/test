[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
[remote "origin"]
	url = https://github.com/Codeception/base.git
	fetch = +refs/heads/*:refs/remotes/origin/*
	pushurl = git@github.com:Codeception/base.git
[branch "2.2"]
	remote = origin
	merge = refs/heads/2.2
[remote "composer"]
	url = https://github.com/Codeception/base.git
	fetch = +refs/heads/*:refs/remotes/composer/*
